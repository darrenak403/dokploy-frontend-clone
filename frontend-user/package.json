{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:analyze": "cross-env ANALYZE=true next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --watch --verbose",
    "validate": "npm run format && npm run format:check && npm run lint:fix && npm run type-check && npm run test:coverage && npm run build",
    "clean": "rm -rf .next out coverage node_modules/.cache"
  },
  "dependencies": {
    "@eslint/js": "^9.35.0",
    "@heroui/input-otp": "^2.1.26",
