    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@reduxjs/toolkit": "^2.9.0",
    "animate.css": "^4.1.1",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.18",
    "dom-to-image-more": "^3.7.1",
    "eslint-plugin-react": "^7.37.5",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.13",
    "globals": "^16.4.0",
    "gsap": "^3.13.0",
    "jiti": "^2.5.1",
    "js-cookie": "^3.0.5",
    "next": "^15.5.3",
    "next-themes": "^0.4.6",
    "pako": "^1.0.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-easy-crop": "^5.5.3",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.93.2",
    "sweetalert2": "^11.23.0",
    "swr": "^2.3.6",
    "three": "^0.180.0",
    "typescript-eslint": "^8.44.0",
    "yup": "^1.7.0"
